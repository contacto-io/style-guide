{
  "name": "@contacto-io/style-guide",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": "git://github.com/contacto-io/contacto-console",
  "version": "0.7.0",
  "main": "build/index.js",
  "module": "build/index.es.js",
  "sideEffects": [
    "*.scss",
    "*.css"
  ],
  "files": [
    "build"
  ],
  "scripts": {
    "build": "pnpm run css-build && rollup -c",
    "watch": "pnpm run css-build && rollup -c -w --local",
    "css-build": "sass --no-source-map src/style/main.scss build/style-guide.css",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build -s static",
    "deploy-storybook": "storybook-to-ghpages",
    "chromatic": "npx chromatic --project-token=b366e708e279",
    "analyze": "source-map-explorer 'build/*.js'"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.14.8",
    "@babel/preset-env": "^7.14.8",
    "@babel/preset-react": "^7.14.5",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^19.0.1",
    "@rollup/plugin-node-resolve": "^13.0.2",
    "@storybook/addon-actions": "^8.0.5",
    "@storybook/addon-essentials": "^8.0.5",
    "@storybook/addon-links": "^8.0.5",
    "@storybook/addon-mdx-gfm": "^8.0.5",
    "@storybook/addons": "^7.6.17",
    "@storybook/blocks": "^8.0.5",
    "@storybook/client-api": "^7.6.17",
    "@storybook/node-logger": "^8.0.5",
    "@storybook/react": "^8.0.5",
    "@storybook/react-vite": "^8.0.5",
    "@storybook/storybook-deployer": "^2.8.16",
    "@storybook/testing-react": "^2.0.1",
    "@storybook/theming": "^8.0.5",
    "@testing-library/react": "^12.0.0",
    "antd": "^4.16.8",
    "cross-env": "^7.0.3",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "libphonenumber-js": "^1.9.20",
    "loadash": "^1.0.0",
    "material-design-icons": "^3.0.1",
    "postcss": "^8.3.6",
    "prop-types": "^15.7.2",
    "raw-loader": "^4.0.2",
    "rc-json-editor": "^2.0.33",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-sortable-hoc": "^2.0.0",
    "rollup": "^2.53.3",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-uglify": "^6.0.4",
    "sass": "^1.74.1",
    "simplebar": "^5.3.5",
    "simplebar-react": "^2.3.5",
    "source-map-explorer": "^2.5.2",
    "storybook": "^8.0.5",
    "ts-dedent": "^2.1.1",
    "vite": "^5.2.8",
    "vite-plugin-svgr": "^4.2.0",
    "wavesurfer.js": "^7.0.3",
    "yeoman-generator": "^5.4.2"
  },
  "peerDependencies": {
    "antd": "^4.16.8",
    "libphonenumber-js": "^1.9.20",
    "prop-types": "^15.7.2",
    "rc-json-editor": "^2.0.33",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-sortable-hoc": "^2.0.0",
    "simplebar": "^5.3.5",
    "simplebar-react": "^2.3.5"
  },
  "dependencies": {
    "@formatjs/intl-displaynames": "^6.6.6",
    "@formatjs/intl-locale": "^3.4.5"
  }
}
